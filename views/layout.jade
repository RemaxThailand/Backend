doctype html
html(lang='en')
	head
		title #{__('Sign in')} | #{config.systemName} #{data.language}
		meta(charset='utf-8')
		meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
		link(rel='stylesheet', href='//src.remaxthailand.co.th/libs/bootstrap/3.3.5/css/bootstrap.min.css')
		link(rel='stylesheet', href='//src.remaxthailand.co.th/libs/font-awesome/4.4.0/css/font-awesome.min.css')
		link(rel='stylesheet', href='//src.remaxthailand.co.th/libs/adminlte/latest/css/AdminLTE.min.css')
		link(rel='stylesheet', href='//src.remaxthailand.co.th/libs/adminlte/latest/css/skins/skin-blue.min.css')

	body.skin-blue

		.wrapper
			header.main-header
				a.logo(href='index2.html')
					span.logo-mini
						b Remax
					span.logo-lg
						b Remax
						| Thailand
				// Header Navbar: style can be found in header.less
				nav.navbar.navbar-static-top(role='navigation')
					// Sidebar toggle button
					a.sidebar-toggle(href='#', data-toggle='offcanvas', role='button')
						span.sr-only Toggle navigation
					// Navbar Right Menu
					.navbar-custom-menu
						ul.nav.navbar-nav
							// Messages: style can be found in dropdown.less
							li.dropdown.messages-menu
								a.dropdown-toggle(href='#', data-toggle='dropdown')
									i.fa.fa-envelope-o
									span.label.label-success 4
								ul.dropdown-menu
									li.header You have 4 messages
									li
										// inner menu: contains the actual data
										ul.menu
											li
												// start message
												a(href='#')
													.pull-left
														img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user2-160x160.jpg', alt='User Image')
													h4
														| Support Team
														small
															i.fa.fa-clock-o
															|	5 mins
													p Why not buy a new awesome theme?
											// end message
											li
												a(href='#')
													.pull-left
														img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user3-128x128.jpg', alt='User Image')
													h4
														| AdminLTE Design Team
														small
															i.fa.fa-clock-o
															|	2 hours
													p Why not buy a new awesome theme?
											li
												a(href='#')
													.pull-left
														img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user4-128x128.jpg', alt='User Image')
													h4
														| Developers
														small
															i.fa.fa-clock-o
															|	Today
													p Why not buy a new awesome theme?
											li
												a(href='#')
													.pull-left
														img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user3-128x128.jpg', alt='User Image')
													h4
														| Sales Department
														small
															i.fa.fa-clock-o
															|	Yesterday
													p Why not buy a new awesome theme?
											li
												a(href='#')
													.pull-left
														img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user4-128x128.jpg', alt='User Image')
													h4
														| Reviewers
														small
															i.fa.fa-clock-o
															|	2 days
													p Why not buy a new awesome theme?
									li.footer
										a(href='#') See All Messages
							// Notifications: style can be found in dropdown.less
							li.dropdown.notifications-menu
								a.dropdown-toggle(href='#', data-toggle='dropdown')
									i.fa.fa-bell-o
									span.label.label-warning 10
								ul.dropdown-menu
									li.header You have 10 notifications
									li
										// inner menu: contains the actual data
										ul.menu
											li
												a(href='#')
													i.fa.fa-users.text-aqua
													|	5 new members joined today
											li
												a(href='#')
													i.fa.fa-warning.text-yellow
													|	Very long description here that may not fit into the
													|											 page and may cause design problems
											li
												a(href='#')
													i.fa.fa-users.text-red
													|	5 new members joined
											li
												a(href='#')
													i.fa.fa-shopping-cart.text-green
													|	25 sales made
											li
												a(href='#')
													i.fa.fa-user.text-red
													|	You changed your username
									li.footer
										a(href='#') View all
							// Tasks: style can be found in dropdown.less
							li.dropdown.tasks-menu
								a.dropdown-toggle(href='#', data-toggle='dropdown')
									i.fa.fa-flag-o
									span.label.label-danger 9
								ul.dropdown-menu
									li.header You have 9 tasks
									li
										// inner menu: contains the actual data
										ul.menu
											li
												// Task item
												a(href='#')
													h3
														| Design some buttons
														small.pull-right 20%
													.progress.xs
														.progress-bar.progress-bar-aqua(style='width: 20%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
															span.sr-only 20% Complete
											// end task item
											li
												// Task item
												a(href='#')
													h3
														| Create a nice theme
														small.pull-right 40%
													.progress.xs
														.progress-bar.progress-bar-green(style='width: 40%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
															span.sr-only 40% Complete
											// end task item
											li
												// Task item
												a(href='#')
													h3
														| Some task I need to do
														small.pull-right 60%
													.progress.xs
														.progress-bar.progress-bar-red(style='width: 60%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
															span.sr-only 60% Complete
											// end task item
											li
												// Task item
												a(href='#')
													h3
														| Make beautiful transitions
														small.pull-right 80%
													.progress.xs
														.progress-bar.progress-bar-yellow(style='width: 80%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
															span.sr-only 80% Complete
											// end task item
									li.footer
										a(href='#') View all tasks
							// User Account: style can be found in dropdown.less
							
									
							li.dropdown.hidden-xs
								a.dropdown-toggle(href="#", data-toggle="dropdown", role="button", aria-expanded="false") #{__('language')} 
									span.caret
								ul.dropdown-menu(role="menu")
									li(class="#{data.language == 'th' ? 'active' : ''}")
										a(href="#{data.language == 'th' ? 'javascript:void(0)' : '/language/th'}", class="#{data.language == 'th' ? 'color-white' : ''}") #{__('languageThai')}
									li(class="#{data.language == 'en' ? 'active' : ''}")
										a(href="#{data.language == 'en' ? 'javascript:void(0)' : '/language/en'}", class="#{data.language == 'en' ? 'color-white' : ''}") #{__('languageEnglish')}


							li.dropdown.user.user-menu
								a.dropdown-toggle(href='#', data-toggle='dropdown')
									img.user-image(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user2-160x160.jpg', alt='User Image')
									span.hidden-xs #{data.memberInfo.displayName}
								ul.dropdown-menu
									// User image
									li.user-header
										img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user2-160x160.jpg', alt='User Image')
										p
											| #{data.memberInfo.displayName} - #{data.memberInfo.memberType}
											small Member since Nov. 2012
									// Menu Body
									li.user-body
										.row
											.col-xs-4.text-center
												a(href='#') Followers
											.col-xs-4.text-center
												a(href='#') Sales
											.col-xs-4.text-center
												a(href='#') Friends
										// /.row
									// Menu Footer
									li.user-footer
										.pull-left
											a.btn.btn-default.btn-flat(href='#') #{__('Profile')}
										.pull-right
											a.btn.btn-default.btn-flat(href='#') #{__('Sign out')}


					block content

			// Left side column. contains the sidebar
			aside.main-sidebar
				// sidebar: style can be found in sidebar.less
				section.sidebar
					// Sidebar user panel
					.user-panel
						.pull-left.image
							img.img-circle(src='//src.remaxthailand.co.th/libs/adminlte/latest/img/user2-160x160.jpg', alt='User Image')
						.pull-left.info
							p #{data.memberInfo.displayName}
							a(href='#')
								i.fa.fa-circle.text-success
								|	#{data.memberInfo.memberType}
					// search form
					form.sidebar-form(action='#', method='get')
						.input-group
							input.form-control(type='text', name='q', placeholder='Search...')
							span.input-group-btn
								button#search-btn.btn.btn-flat(type='submit', name='search')
									i.fa.fa-search
					// /.search form
					// sidebar menu: : style can be found in sidebar.less
					ul.sidebar-menu
						li.header MAIN NAVIGATION
						for menu, key in data.menu
							li(class="#{menu.hasChild == 1 ? 'treeview' : ''}")
								a(href="#{menu.link}")
									i(class="fa fa-#{menu.icon}") 
									span &nbsp;#{__('screen-'+key+'-header')}
									if menu.hasChild == 1
										i.fa.fa-angle-left.pull-right

						li.treeview
							a(href='#')
								i.fa.fa-dashboard
								span Dashboard
								i.fa.fa-angle-left.pull-right
							ul.treeview-menu
								li
									a(href='../../index.html')
										i.fa.fa-circle-o
										|	Dashboard v1
								li
									a(href='../../index2.html')
										i.fa.fa-circle-o
										|	Dashboard v2

				// /.sidebar
			// ===============================================
			// Content Wrapper. Contains page content
			.content-wrapper
				// Content Header (Page header)
				section.content-header
					h1
						| หน้าหลัก
						small it all starts here
					ol.breadcrumb
						li
							a(href='#')
								i.fa.fa-dashboard
								|	Home
						li
							a(href='#') Examples
						li.active Blank page
				// Main content
				section.content
					// Default box
					.box
						.box-header.with-border
							h3.box-title Title
							.box-tools.pull-right
								button.btn.btn-box-tool(type='button', data-widget='collapse', data-toggle='tooltip', title='Collapse')
									i.fa.fa-minus
								button.btn.btn-box-tool(type='button', data-widget='remove', data-toggle='tooltip', title='Remove')
									i.fa.fa-times
						.box-body
							| Start creating your amazing application!
						// /.box-body
						.box-footer
							| Footer
						// /.box-footer
					// /.box
				// /.content
			// /.content-wrapper
			footer.main-footer
				.pull-right.hidden-xs
					b Version
					|	2.3.2
				strong
					| Copyright © 2014-2015 
					a(href='http://almsaeedstudio.com') Almsaeed Studio
					| .
				|	All rights
				|		 reserved.

			.control-sidebar-bg

		script(src='//src.remaxthailand.co.th/libs/jquery/2.1.4/jquery.min.js')
		script(src='//src.remaxthailand.co.th/libs/js-cookie/2.0.3/js.cookie.min.js')
		script(src='//src.remaxthailand.co.th/libs/bootstrap/3.3.5/js/bootstrap.min.js')
		script(src='//src.remaxthailand.co.th/libs/adminlte/latest/js/app.min.js')
		- if (locals.script)
			script(src=script)